# Generated by TortoiseHg settings dialog


[tortoisehg]
editor = /usr/bin/gvim
vdiff = _gvimdiff

issue.regex = \b\w{2,7}-\d+\b
changeset.link =
issue.link = https://infinica.atlassian.net/browse/{0}

[merge-tools]
diffconflicts.executable = diffconflicts
diffconflicts.args = $base $local $other $output
diffconflicts.priority = 1
diffconflicts.premerge = keep

[extensions]
mq = 
mercurial_keyring=
rebase = 
graphlog =
extdiff = 

[extdiff]
cmd._gvimdiff = gvimdiff
opts._gvimdiff = $parent1 $child $parent2

[ui]
username = Bogdan Mustiata <bogdan.mustiata@gmail.com>
merge = diffconflicts
style=compact

[alias]
slog = log --style=/home/raptor/projects/mercurial-cli-templates/map-cmdline.slog
nlog = log --style=/home/raptor/projects/mercurial-cli-templates/map-cmdline.nlog
sglog = glog --style=/home/raptor/projects/mercurial-cli-templates/map-cmdline.sglog

